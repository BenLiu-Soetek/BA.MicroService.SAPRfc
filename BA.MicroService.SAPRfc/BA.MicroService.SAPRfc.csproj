    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b95e6d1b-66f9-4daf-99a6-1c5b40bc0e85</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
