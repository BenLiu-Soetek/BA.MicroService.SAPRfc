  </ItemGroup>

  <ItemGroup>
    <Reference Include="cpc4n">
      <HintPath>Lib\SAP\cpc4n.dll</HintPath>
    </Reference>
    <Reference Include="sapnco">
      <HintPath>Lib\SAP\sapnco.dll</HintPath>
    </Reference>
    <Reference Include="sapnco_utils">
      <HintPath>Lib\SAP\sapnco_utils.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
